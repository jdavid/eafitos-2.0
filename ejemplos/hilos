; Programa que crea dos hilos, el padre imprime una "p" y el hijo una "h"

CODIGO
	cargar_i @1, #31	; número del servicio

	; crea un hilo
	cargar_i @2, hilo
	apilar @2
	cargar_i @3, #1
	apilar @3
	ser_sis

	; escribe el caracter "p"
	cargar_i @2, #112
	apilar @2
	apilar @1
	ser_sis

	; termina el padre
	saltar fin


hilo	; Codigo del hijo
	; escribe el caracter "h"
	cargar_i @2, #104
	apilar @2
	apilar @1
	ser_sis
	
fin
